{
    "name": "{{npmName}}",
    "version": "{{npmVersion}}",
    "description": "NodeJS client for {{npmName}}",
    "repository": {
        "type": "git",
        "url": "https://{{gitHost}}/{{gitUserId}}/{{gitRepoId}}.git"
    },
    "main": "dist/api.js",
    "types": "dist/api.d.ts",
    "scripts": {
        "clean": "rm -Rf node_modules/ *.js",
        "build": "tsc",
        "test": "vitest"
    },
    "author": "OpenAPI-Generator Contributors",
    "license": "Unlicense",
    "dependencies": {
        "bluebird": "^3.5.0",
        "request": "^2.81.0",
        "rewire": "^3.0.2"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.33",
        "@types/node": "^12",
        "@types/request": "^2.48.8",
        "typescript": "^4.0",
        "vitest": "^1.1.1",
        "dotenv": "^16.3.1"
    }{{#npmRepository}},
    "publishConfig": {
        "registry": "{{npmRepository}}"
    }{{/npmRepository}}
}
